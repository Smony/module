{
    "name": "smony/module",
    "description": "Generate modules",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^11.0",
        "spatie/laravel-package-tools": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Smony\\Module\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smony\\Module\\Providers\\ModuleServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "license": "MIT",
    "version": "1.1.0"
}
